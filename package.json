{
  "name": "eletronapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./disteletron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel devr deve",
    "devr": "vite",
    "deve": " npm run  transpile && cross-env NODE_ENV=development electron .",
    "transpile": " tsc --project src/eletron/tsconfig.json",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "dist:mac": "npm run  transpile && npm run build &&   electron-builder --mac --arm64",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "chart.js": "^4.5.1",
    "cross-env": "^10.1.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.1.0",
    "react-draggable": "^4.5.0",
    "react-use": "^17.6.0",
    "tailwindcss": "^4.1.18",
    "three": "^0.182.0",
    "transpile": "^2.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^4.4.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "os-utils": "^0.0.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
